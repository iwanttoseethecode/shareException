package tip17;

import java.util.Date;

public class Sub extends Super {
	private Sub instance = new Sub();
	private final  Date date;  
	  
	  
    public Sub() {  
        date = new Date();  
        
    }  

  
	public static Sub instance(){
//		date = new Date();
		
		return instance;
	}
  
    public void method() {  
        System.out.println(date);  
    }  
  
  
    public static void main(String[] args) {  
        Sub s = new Sub();  
        s.method();  
    }  
}
